{
  "name": "satellite-gis-infrastructure",
  "version": "1.0.0",
  "description": "AWS CDK infrastructure for Satellite GIS Platform",
  "main": "bin/satellite-gis.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "deploy": "cdk deploy --all",
    "deploy:dev": "cdk deploy --all --context environment=dev",
    "deploy:staging": "cdk deploy --all --context environment=staging",
    "deploy:prod": "cdk deploy --all --context environment=prod",
    "diff": "cdk diff",
    "synth": "cdk synth"
  },
  "keywords": ["aws", "cdk", "satellite", "gis"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "aws-cdk": "^2.110.0"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.110.0",
    "constructs": "^10.3.0"
  }
}
